<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_layout_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_push"
    android:orientation="vertical"
    android:background="@color/common_signin_btn_light_text_focused"
    tools:context="com.diogo.mobile.app.PushActivity">

    <EditText
        android:id="@+id/acti_push_editText_mensagem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="TextFields"/>
</LinearLayout>
